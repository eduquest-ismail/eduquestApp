<?xml version='1.0' encoding='utf-8'?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
     xmlns:gap   = "http://phonegap.com/ns/1.0"
     id          = "com.phonegap.eduquest"
     versionCode = "10"
     version     = "1.0.0" >
  <name>Eduquest Mobile</name>
  <description>
    Eduquest Mobile Application
  </description>
  <author email="ismail@eduqestindia.in" href="www.eduquestindia.in">
    Mohamed Ismail
  </author>
  <gap:platform name="android" />
  <gap:plugin name = "org.apache.cordova.splashscreen" />
  <preference name="AutoHideSplashScreen" value="false" />
  <preference name="SplashScreenDelay" value="3000" />
  <preference name="permissions" value="none"/>
  <feature name="http://api.phonegap.com/1.0/network"/>
  <gap:splash src="images/320x470_72.png" />
  <preference name="Fullscreen" value="true" />
  <preference name="BackgroundColor" value="0xfff7f7f7"/>
  <content src="index.html" />
  <access origin="*" />
</widget>
